{
    "extends": "airbnb",
    "plugins": [
        "react",
        "import",
        "jest"
    ],
    "env": {
      "browser": true,
      "jest/globals": true
    },
    "rules": {
      "react/jsx-filename-extension": "off",
      "react/prefer-stateless-function": [2, { "ignorePureComponents": true }]
    }
}
